<head>
    <title><%= @getPreparedTitle() %></title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="description" content="<%= @site.description %>" />
    <meta name="keywords" content="adventure time,cartoon network,cartoons,finn,jake,princess bubblegum" />
    
    <%- @getBlock("styles").add(['/css/skel.css','/css/style.css','/css/style-desktop.css']).toHTML() %>
    <%- @getBlock("scripts").add(['/js/jquery.min.js','/js/skel.min.js','/js/skel-layers.min.js','/js/init.js']).toHTML() %>
    <link rel="alternative" href="/rss.xml" title="<%= @site.title %>" type="application/atom+xml">
</head>